        {% block javascripts %}
            <script src="../front/Js/jquery-3.5.1.js"></script>
            <script src="../front/Js/bootstrap.min.js"></script>
            <script src="../front/Js/popper.min.js"></script>
            <script src="../front/Js/dashboard.js"></script>
        {% endblock %}
