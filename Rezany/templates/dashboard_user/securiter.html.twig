{% extends 'base.html.twig' %}

{% block title %}Hello DashboardUserController!{% endblock %}

{% block stylesheets %}

    <link rel="stylesheet" href="../front/Css/Dashbord.css" />
    <link rel="stylesheet" href="../front/Css/Footer.css" />



{% endblock %}

{% block body %}
    <section class="container-fluid">
        <div class="nav-btn">Menu</div>
        <div class="sidebar">
            <div class="nav-dashbord">
                <ul>
                    <li class="active"><a href="DashboardUser.html">Dashboard</a></li>
                    <li><a href="DashboardHistory.html">Historique</a></li>
                    <li><a href="DashboardNotif.html">Notification</a></li>
                    <li><a href="DashboardSecu.html">Confidentialité / sécurité</a></li>
                </ul>
            </div>
        </div>
        <div class="container">
            <div class="main-dashbord">
                <h1 class="mt-4">Dashboard Compte / sécurité</h1>
                <div class="panel-wrapper">
                    <div class="panel-head">Nom</div>
                    <div class="input-group mb-3">
                        <input
                                type="text"
                                class="form-control"
                                placeholder="Nom actuelle"
                                aria-label="Nom actuelle"
                                aria-describedby="basic-addon2"
                        />
                        <div class="input-group-prepend">
                            <button class="btn btn-outline-secondary" type="button">
                                Validation
                            </button>
                        </div>
                    </div>
                </div>
                <div class="panel-wrapper">
                    <div class="panel-head">Email</div>
                    <div class="input-group mb-3">
                        <input
                                type="email"
                                class="form-control"
                                placeholder="Email actuelle"
                                aria-label="Email actuelle"
                                aria-describedby="basic-addon2"
                        />
                        <div class="input-group-prepend">
                            <button class="btn btn-outline-secondary" type="button">
                                Validation
                            </button>
                        </div>
                    </div>
                </div>
                <div class="panel-wrapper">
                    <div class="panel-head">Mot de passe</div>
                    <div class="input-group mb-3">
                        <input
                                type="password"
                                class="form-control"
                                placeholder="Mot de passe actuelle"
                                aria-label="Mot de passe actuelle"
                                aria-describedby="basic-addon2"
                        />
                        <div class="input-group-prepend">
                            <button class="btn btn-outline-secondary" type="button">
                                Validation
                            </button>
                        </div>
                    </div>
                </div>
                <div class="panel-wrapper">
                    <div class="panel-head">Changer d mot de passe</div>
                    <div class="input-group mb-3">
                        <input
                                type="password"
                                class="form-control"
                                placeholder="Nouveau mot de passe"
                                aria-label="Nouveau mot de passe"
                                aria-describedby="basic-addon2"
                        />
                        <div class="input-group-prepend">
                            <button class="btn btn-outline-secondary" type="button">
                                Validation
                            </button>
                        </div>
                    </div>
                </div>
                <h3 class="mt-4">Détail du compte</h3>

                <div class="panel-wrapper">
                    <div class="panel-head">Nom</div>
                    <div class="input-group mb-3">
                        <input
                                type="text"
                                class="form-control"
                                placeholder="Nom"
                                aria-label="Nom"
                                aria-describedby="basic-addon2"
                        />
                        <div class="input-group-prepend">
                            <button class="btn btn-outline-secondary" type="button">
                                Validation
                            </button>
                        </div>
                    </div>
                </div>
                <div class="panel-wrapper">
                    <div class="panel-head">Prénom</div>
                    <div class="input-group mb-3">
                        <input
                                type="text"
                                class="form-control"
                                placeholder="prenom"
                                aria-label="prenom"
                                aria-describedby="basic-addon2"
                        />
                        <div class="input-group-prepend">
                            <button class="btn btn-outline-secondary" type="button">
                                Validation
                            </button>
                        </div>
                    </div>
                </div>
                <div class="panel-wrapper">
                    <div class="panel-head">Adresse</div>
                    <div class="input-group mb-3">
                        <input
                                type="text"
                                class="form-control"
                                placeholder="Adresse"
                                aria-label="Adresse"
                                aria-describedby="basic-addon2"
                        />
                        <div class="input-group-prepend">
                            <button class="btn btn-outline-secondary" type="button">
                                Validation
                            </button>
                        </div>
                    </div>
                </div>
                <div class="panel-wrapper">
                    <div class="panel-head">Code postal</div>
                    <div class="input-group mb-3">
                        <input
                                type="number"
                                class="form-control"
                                placeholder="cp"
                                aria-label="cp"
                                aria-describedby="basic-addon2"
                        />
                        <div class="input-group-prepend">
                            <button class="btn btn-outline-secondary" type="button">
                                Validation
                            </button>
                        </div>
                    </div>
                </div>
                <div class="panel-wrapper">
                    <div class="panel-head">Département</div>
                    <div class="input-group mb-3">
                        <input
                                type="text"
                                class="form-control"
                                placeholder="département"
                                aria-label="département"
                                aria-describedby="basic-addon2"
                        />
                        <div class="input-group-prepend">
                            <button class="btn btn-outline-secondary" type="button">
                                Validation
                            </button>
                        </div>
                    </div>
                </div>
                <div class="panel-wrapper">
                    <div class="panel-head">Ville</div>
                    <div class="input-group mb-3">
                        <input
                                type="text"
                                class="form-control"
                                placeholder="ville"
                                aria-label="ville"
                                aria-describedby="basic-addon2"
                        />
                        <div class="input-group-prepend">
                            <button class="btn btn-outline-secondary" type="button">
                                Validation
                            </button>
                        </div>
                    </div>
                </div>
                <div class="from-row from-group modification">
                    <a href="../Home.html">Modification</a>
                </div>
                <h3 class="mt-4">Supprimer le compte</h3>
                <div class="from-row from-group modification">
                    <a href="../Home.html">Supprimer</a>
                </div>
            </div>
        </div>
    </section>

{% endblock %}